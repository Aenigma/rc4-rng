{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2015"],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "erasableSyntaxOnly": true,
    "skipLibCheck": true
  }
}
